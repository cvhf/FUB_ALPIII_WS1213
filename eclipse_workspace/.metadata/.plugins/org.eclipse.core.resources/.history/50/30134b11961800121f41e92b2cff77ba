// Klasse zum Testen des Stacks

public class StapelTest extends ArrayStapel{
		

	public static void main(String[] args) {
		
		int[] arrayOfInts;
		arrayOfInts = new int[3];
		arrayOfInts[0]= 1;
		arrayOfInts[1]= 1;
		arrayOfInts[2]= 1;
		
		
		// Stapel erstellen (Array der Länge 1 wird automatisch erstellt)
		ArrayStapel Stapel;
		Stapel= new ArrayStapel(1);
		
		// Sta[el füllen
		Stapel.push("Eins");
		Stapel.push("Zwei");
		
		// noch einen Stapel aus bereits vorhandenem Array 'TestArray' erstellen
		ArrayStapel Stapel2= new ArrayStapel(arrayOfInts);
		
		// Stapel auffüllen
		Stapel2.push("Drei");
		
		
		try{
			System.out.println(Stapel.pop());
			System.out.println(Stapel2.pop());
		}catch (EmptyStackException es){
			System.out.println("Schlecht!");
		}
		

	}

}
